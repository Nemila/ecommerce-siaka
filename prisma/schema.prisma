generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Commande {
  id        String @id @default(uuid())
  nom       String
  prenom    String
  email     String
  telephone String
  address   String
  produitId String
  prix      Float

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
